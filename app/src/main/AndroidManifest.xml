<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team03.coursewarehub" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Search.SearchListActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".Search.SearchDisplayActivity"
            android:label="@string/title_activity_search_second" />
        <activity android:name=".Youtube.VideoDisplay" >
        </activity>
        <activity android:name=".Example.ExampleListActivity" >
        </activity>
        <activity android:name=".Assignments.AssignmentListActivity" >
        </activity>

    </application>

</manifest>
